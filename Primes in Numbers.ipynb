   "source": [
    "def largest_prime(n):\n",
    "    i = 2\n",
    "    \n",
    "    while i ** i <= n:\n",
    "        if n % i:\n",
    "            i += 1\n",
    "        else:\n",
    "            n //= i\n",
    "            \n",
    "    return n"
   ]
